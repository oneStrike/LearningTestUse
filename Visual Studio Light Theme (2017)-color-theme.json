{
  "name": "Visual Studio Light Theme (2017)",
  "type": "light",
  "colors": {
    // Activity Bar
    "activityBar.background": "#000000",
    "activityBar.foreground": "#41a863",
    "activityBar.border": "#807f7f",
    "activityBar.inactiveForeground": "#8ea6b6",

    // Editor
    "editor.background": "#cce8cf",
    "editor.foreground": "#000000",
    "editor.selectionBackground": "#0078d775",
    "editor.inactiveSelectionBackground": "#bfcddb75",
    "editor.lineHighlightBorder": "#eaeaf2",
    "editor.findMatchBackground": "#949f84",
    "editor.findMatchHighlightBackground": "#f6b94d",
    "editorLineNumber.foreground": "#2b91af",
    "editorLineNumber.activeForeground": "#2b91af",
    "editorCursor.foreground": "#e50000",
    "editorBracketMatch.background": "#fa5e00",
    "editorBracketMatch.border": "#00000000",
    "editorGroupHeader.noTabsBackground": "#cce8cf",
    "editorGroupHeader.tabsBackground": "#fffae8",
    "editorGroupHeader.tabsBorder": "#cce8cf",
    "editorGroup.border": "#fffae8",
    "editorGroup.dropBackground": "#add6ff",

    // Inputs
    "input.foreground": "#000000",
    "input.border": "#00000000",
    "input.placeholderForeground": "#717171",
    "inputValidation.errorForeground": "#ffffff",
    "inputValidation.errorBorder": "#e51400",

    // Menu
    "menubar.selectionBorder": "#00000000",
    "menubar.selectionForeground": "#000000",
    "menubar.selectionBackground": "#fffae8",
    "menu.foreground": "#000000",
    "menu.background": "#fffae8",
    "menu.selectionForeground": "#000000",
    "menu.selectionBackground": "#ffb64e",

    // Scrollbar
    "scrollbarSlider.background": "#c2c3c9",
    "scrollbarSlider.activeBackground": "#5b5b5b",
    "scrollbarSlider.hoverBackground": "#686868",

    // Sidebar
    "sideBar.background": "#c5c5c5",
    "sideBar.foreground": "#000000",
    "sideBar.border": "#c5c5c5",
    "sideBarTitle.foreground": "#000000",
    "sideBarSectionHeader.foreground": "#41a863",
    "sideBarSectionHeader.background": "#41a863",
    "sideBarSectionHeader.border": "#00000000",

    // Status Bar
    "statusBar.background": "#41a863",
    "statusBar.debuggingBackground": "#ca5100",
    "statusBar.noFolderBackground": "#68217a",

    // Tabs
    "tab.activeForeground": "#cce8cf",
    "tab.activeBackground": "#41a863",
    "tab.activeBorder": "#41a863",
    "tab.hoverBackground": "#41a863",
    "tab.inactiveForeground": "#000000",
    "tab.inactiveBackground": "#00000000",
    "tab.border": "#00000000",
    "tab.unfocusedActiveForeground": "#41a863",
    "tab.unfocusedInactiveForeground": "#000000",
    "tab.unfocusedHoverBackground": "#41a863",

    // Terminal
    "terminal.background": "#ffffff",
    "terminal.border": "#cccedb",
    "terminal.foreground": "#0c0c0c",
    "terminalCursor.background": "#ffffff",
    "terminalCursor.foreground": "#0c0c0c",

    // Titlebar
    "titleBar.activeForeground": "#000000",
    "titleBar.activeBackground": "#fffae8",
    "titleBar.inactiveBackground": "#fffae8",
    "titleBar.border": "#cccedb"
  },
  "tokenColors": "./Visual Studio.tmTheme"
}
